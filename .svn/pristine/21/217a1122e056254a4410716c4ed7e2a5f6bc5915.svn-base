			page		:	page
		};
		
		$.ajax({
			type:"POST",
			url:"/admin/doctor_index_manage/pressure_list.go",
			dataType:"html",
			data:param,
			success:function(msg){
				$("#contents-list").html(msg);
			}
		});
		return false;
	}
	

	

